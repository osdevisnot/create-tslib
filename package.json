{
  "name": "create-tslib",
  "version": "0.0.0",
  "private": true,
  "description": "Maximum Overkill for Typescript Library Authors",
  "keywords": [
    "create-tslib"
  ],
  "repository": "osdevisnot/create-tslib",
  "license": "MIT",
  "author": "osdevisnot <osdevisnot@gmail.com>",
  "scripts": {
    "dev": "cd packages/tslib-cli && pnpm pack",
    "postdev": "cd packages/create-tslib && pnpm link",
    "postinstall": "pnpm run dev",
    "prepare": "pnpm recursive install",
    "pub": "pnpm recursive exec yarn publish",
    "presetup": "git clean -fdX",
    "setup": "pnpm install --prefer-offline",
    "sort": "pnpm run sort-package-json",
    "postsort": "pnpm recursive exec 'sort-package-json'",
    "sort-package-json": "sort-package-json"
  },
  "devDependencies": {
    "sort-package-json": "^1.22.1"
  }
}
